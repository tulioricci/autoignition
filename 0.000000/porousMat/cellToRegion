FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000000/porousMat";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    s_frontAndBack
    {
        type            empty;
    }
    s_side
    {
        type            zeroGradient;
    }
    porousMat_to_flow
    {
        type            calculated;
        value           uniform 0;
    }
}


