FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000000/flow";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    f_frontAndBack
    {
        type            empty;
    }
    f_side
    {
        type            zeroGradient;
    }
    flow_to_porousMat
    {
        type            calculated;
        value           uniform 0;
    }
}


